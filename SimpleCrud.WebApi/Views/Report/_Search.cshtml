@model STSearchVM

<div class="row">
    <div class="col-md-12">
        <form method="post" asp-action="Search" onsubmit="return jQueryAjaxPost(this);">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="GirishTarih"></label>
                <input asp-for="GirishTarih" class="form-control" />
            </div>
            <div class="form-group">
                <label asp-for="CikishTarih"></label>
                <input asp-for="CikishTarih" class="form-control" />
            </div>
            <div class="form-group">
                <label asp-for="MalCode"></label>
                <select asp-for="MalCode" asp-items="@Model.STKList" class="form-control"></select>
            </div>
            <div class="form-group">
                <div class="col-md-6 offset-md-3">
                    <input type="submit" value="Submit" class="btn btn-primary btn-block" />
                </div>
            </div>
        </form>
    </div>
</div>